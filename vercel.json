{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "builds": [
        {
            "src": "src/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "450mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.py"
        }
    ]
}