{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 5000",
    "builds": [
        {
            "src": "src/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "150mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/main.py"
        }
    ]
}